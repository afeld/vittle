{
  "manifest_version": 2,

  "name": "Vittle",
  "description": "Shows previews of items on food ordering sites.",
  "version": "1.0",

  "content_scripts": [
    {
      "matches": ["*://*.seamless.com/*"],
      "js": ["components/underscore/underscore.js", "components/zepto/zepto.js", "src/vittle.js", "src/content.js"]
    }
  ],
  "permissions": [
    "*://*.seamless.com/"
  ]
}
